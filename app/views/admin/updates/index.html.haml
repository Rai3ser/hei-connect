- model_class = Update
.content-box
  .content-box-header
    =t '.title', :default => model_class.model_name.human.pluralize
  .padded
    = render :partial => 'admin/shared/updates_table', locals: { updates: @updates }
    %br
    = link_to t('.new', :default => t("helpers.links.new")), new_admin_update_path, :class => 'btn btn-primary'
