- model_class = UserSession
%table.table.table-bordered.table-striped
  %thead
    %tr
      %th= model_class.human_attribute_name(:id)
      %th= model_class.human_attribute_name(:year)
      %th= model_class.human_attribute_name(:try)
      %th= model_class.human_attribute_name(:absences_session)
      %th= model_class.human_attribute_name(:grades_session)
      %th= model_class.human_attribute_name(:update_number)
      %th= model_class.human_attribute_name(:user_id)
      %th= model_class.human_attribute_name(:created_at)
      %th=t '.actions', :default => t("helpers.actions")
  %tbody
    - user_sessions.each do |user_session|
      %tr
        %td= link_to user_session.id, admin_user_session_path(user_session)
        %td= user_session.year
        %td= user_session.try
        %td= user_session.absences_session
        %td= user_session.grades_session
        %td= user_session.update_number
        %td= user_session.user_id
        %td=l user_session.created_at
        %td
          = link_to t('.edit', :default => t("helpers.links.edit")), edit_admin_user_session_path(user_session), :class => 'btn btn-mini'
          = link_to t('.destroy', :default => t("helpers.links.destroy")), admin_user_session_path(user_session), :method => :delete, :data => { :confirm => t('.confirm', :default => t("helpers.links.confirm", :default => 'Are you sure?')) }, :class => 'btn btn-mini btn-danger'