- model_class = Section
.content-box
  .content-box-header
    =t '.title', :default => model_class.model_name.human.pluralize
  .padded
    = render :partial => 'admin/shared/sections_table', locals: { sections: @sections }
    %br
    .center
      = will_paginate @courses
    = link_to t('.new', :default => t("helpers.links.new")), new_admin_section_path, :class => 'btn btn-primary'
